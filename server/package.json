{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -w src --ext ts --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "db:test:down": "docker compose -f ../docker-compose.test.yml down -v",
    "db:test:up": "docker compose -f ../docker-compose.test.yml up --build -d --wait",
    "test": "yarn run db:test:up && jest",
    "testGA": "yarn jest",
    "posttest": "yarn run db:test:down",
    "test-watch": "yarn run db:test:up && jest --watchAll",
    "posttest-watch": "yarn run db:test:down"
  },
  "dependencies": {
    "@apollo/client": "^3.8.9",
    "@apollo/server": "^4.7.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.2",
    "@types/stack-utils": "^2.0.3",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^2.0.0-beta.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
